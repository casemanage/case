<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
  
  
  
<generatorConfiguration>   
       
    <context id="mysql_tables" targetRuntime="MyBatis3">  
    	
        <commentGenerator>
        	<property name="suppressAllComments" value="true" />
        </commentGenerator>
        
        <jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver" 
        connectionURL="jdbc:sqlserver://127.0.0.1:1433;DatabaseName=case"  
         userId="sa" password="123456"/>  
         
        <!-- 指定生成的类型为java类型，避免数据库中number等类型字段 -->  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>  
        
        
        
        <!-- 生成model模型，对应的包，存放位置可以指定具体的路径,如/ProjectName/src，也可以使用MAVEN来自动生成 -->  
        <javaModelGenerator targetPackage="com.security.manage.model" targetProject="src\main\java">  
            <property name="enableSubPackages" value="true"/>  
        </javaModelGenerator>  
          
        <!--对应的xml mapper文件  	-->
        <sqlMapGenerator  targetPackage="/mybatis/mapping/" targetProject="src\main\resources">  
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>  
          
          
        <!-- 对应的dao接口  --> 
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.security.manage.dao" targetProject="src\main\java">  
            <property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>  
          
     
        <table tableName="MC_ASSOCIATE" domainObjectName="Associate"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
     
        <table tableName="MC_ASSOCIATE_PERSON" domainObjectName="AssociatePerson"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
     
        <table tableName="MC_ASSOCIATE_PLAN" domainObjectName="AssociatePlan"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>   	
     
        <table tableName="MC_ASSOCIATE_TYPE" domainObjectName="AssociateType"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>   	
     
        <table tableName="MC_PERSON" domainObjectName="Person"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
     
        <table tableName="MC_PERSON_TYPE" domainObjectName="PersonType"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
     
        <table tableName="MC_PERSON_LEVEL" domainObjectName="PersonLevel"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
	       
	       <table tableName="MC_PERSON_CAR" domainObjectName="PersonCar"
           enableSelectByExample="false"
	       enableDeleteByExample="false"
	       enableCountByExample="false"
	       enableUpdateByExample="false"/>  
	        
    </context>  
</generatorConfiguration>  
